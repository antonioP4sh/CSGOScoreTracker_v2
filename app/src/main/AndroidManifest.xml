<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.android.counter_strikegoscoretracker">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TeamSelector">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.android.counter_strikegoscoretracker.ScoreTracker"
                  android:label="Score"
                  android:theme="@style/AppThemeLand"
            ></activity>
        <activity android:name="com.example.android.counter_strikegoscoretracker.FinalScore"
                  android:label="Final Score">
            ></activity>
    </application>

</manifest>